<Window x:Class="isaiev_ekz_sp.Window2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:isaiev_ekz_sp"
        mc:Ignorable="d"
        Title="Window2" Height="600" Width="400" Loaded="Window_Loaded" >
    <Grid>
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="40"></RowDefinition>
                <RowDefinition Height="40"></RowDefinition>
                <RowDefinition ></RowDefinition>
            </Grid.RowDefinitions>

            <Button Name="go" Margin="10" Grid.Row="0" Click="go_Click">Go</Button>
            <Button Name="ok" Margin="10" Grid.Row="1" Click="ok_Click">OK!</Button>
            <ListBox Name="res" Margin="10" Grid.Row="2" SelectionChanged="res_SelectionChanged" MouseDoubleClick="res_MouseDoubleClick"></ListBox>
        </Grid>
    </Grid>
</Window>
